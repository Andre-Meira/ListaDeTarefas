@{
    ViewData["Title"] = "Nova Tarefa";
}

<h1>Novo Usuario</h1>

<form asp-action="PossuiConta" method="get">
    <div class="form-group">
        <label class="control-label">Tarefa</label>
        <input name="UserName" class="form-control">
    </div>

    <div class="form-group">
        <label class="control-label">Descricao</label>
        <input name="Senha" class="form-control">
    </div>


    <input type="submit" value="Salvar" class="btn" />
</form>

<a asp-action="Index" class="btn btn-lik">Voltar</a>


@if (ViewBag.Alerta != null)
{
    <script type="text/javascript">
        alert('@ViewBag.Alerta');
    </script>
}